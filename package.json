  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "precommit": "prettier --write --loglevel error . && next lint --quiet --fix && tsc --noEmit --pretty false && echo \"\\n‚úÖ \\033[32mAll checks passed successfully!\\033[0m\\n   \\033[90m- Prettier formatting\\033[0m\\n   \\033[90m- ESLint checks\\033[0m\\n   \\033[90m- TypeScript compilation\\033[0m\\n\"",
    "prepush": "TEST_SILENT=1 vitest run && pnpm build && echo \"\\n‚úÖ \\033[32mAll checks passed successfully!\\033[0m\\n   \\033[90m- Vitest tests\\033[0m\\n   \\033[90m- Next.js build\\033[0m\\n\"",
    "precheck": "echo \"\\nüîç \\033[34mRunning all checks (format, lint, typecheck, test, build)...\\033[0m\\n\" && pnpm precommit && pnpm prepush && echo \"\\nüéâ \\033[32mPrecheck complete! All checks passed.\\033[0m\\n\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:silent": "TEST_SILENT=1 vitest run",
    "test:silent:watch": "TEST_SILENT=1 vitest",
    "test:silent:coverage": "TEST_SILENT=1 vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
