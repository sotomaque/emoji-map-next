    "changeset:create": "node scripts/create-changeset.js",
    "changeset:version": "changeset version",
    "clean": "rm -rf node_modules/.vite && rm -rf .next && rm -rf node_modules",
    "inngest": "pnpm dlx inngest-cli@latest dev"
  },
